{
  "kind": "collectionType",
  "collectionName": "portfolio",
  "info": {
    "name": "Portfolio v4",
    "description": ""
  },
  "options": {
    "increments": true,
    "populateCreatorFields": false,
    "timestamps": false,
    "draftAndPublish": false
  },
  "attributes": {
    "slug": {
      "type": "string"
    },
    "language": {
      "type": "enumeration",
      "enum": [
        "en",
        "fr"
      ]
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "personalDescription": {
      "type": "richtext"
    },
    "professionalDescription": {
      "type": "richtext"
    },
    "reference": {
      "type": "uid"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "categories": {
      "collection": "category",
      "via": "categoriesPortfolio",
      "dominant": true
    },
    "technologies": {
      "collection": "category",
      "via": "technologiesPortfolio",
      "dominant": true
    },
    "projects": {
      "collection": "project",
      "via": "portfolios",
      "dominant": true
    }
  }
}
