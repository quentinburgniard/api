{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "projects",
  "info": {
    "name": "project"
  },
  "options": {
    "increments": true,
    "timestamps": false
  },
  "attributes": {
    "launchDate": {
      "type": "date"
    },
    "name": {
      "type": "string",
      "maxLength": 25
    },
    "url": {
      "type": "string"
    },
    "image": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload"
    },
    "shortDescription": {
      "type": "richtext"
    },
    "longDescription": {
      "type": "richtext"
    },
    "technicalDescription": {
      "type": "richtext"
    }
  }
}
